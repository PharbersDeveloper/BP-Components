{{#bp-layout direction=direction height=height}}
	{{#each rowModel.panels as |panel index|}}
		{{#each conditions as |condition cindex|}}
			{{#if (bp-eq index cindex)}}
				{{bp-panel
					eid=panel.id 
					panelModel=panel 
					time=time 
					condition=condition
					outsideDealedData=outsideDealedData
					onDataReady=(action onQueryReady)}}
			{{/if}}
		{{/each}}
	{{/each}}
{{/bp-layout}}